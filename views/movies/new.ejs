<%- include('../partials/header') %>
<p>Please first ensure that the ticket is not in the dropdown
  <select>
    <% movie.forEach(function(m) { %>
      <option><%= m.title %></option>
    <% }) %>
  </select>
</p>
<form id="new-form" action="/movies" method="POST">
  <label>Title:</label>
  <input type="text" name="title">
  <label>Release Year:</label>
  <input type="text" name="releaseYear">
  <label>MPAA Rating</label>
  <select name="mpaaRating">
    <option value="G">G</option>
    <option value="PG">PG</option>
    <option value="PG-13">PG-13</option>
    <option value="R">R</option>
  </select>
  <label>Movie Link:</label>
  <input type="text" name="movieLink">
  <label>Genre/Genres:</label>
  <input type="text" name="genre">
  <label>Where to Watch:</label>
  <input type="text" name="whereToWatch">
  <input type="submit" value="Add Movie">
</form>

<%- include('../partials/footer') %>